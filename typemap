# $Id: typemap 109 2008-07-25 08:55:33Z rplessl $
TYPEMAP
HWND         T_HANDLE
DWORD        T_IV

################################################################################
INPUT
T_HANDLE
    if(SvROK($arg)) {
        SV** out=hv_fetch((HV*)SvRV($arg), \"-handle\", 7, 0);
        if(out != NULL)
            $var = INT2PTR($type,SvIV(*out));
        else
            $var = NULL;
    } else
       $var = INT2PTR($type,SvIV($arg));

################################################################################
OUTPUT
T_HANDLE
    sv_setiv($arg, PTR2IV($var));
